<?xml version="1.0" encoding="utf-8" ?>
<manifest>
  <control namespace="mdt" constructor="LookupAttributeSelector" version="1.0.2" display-name-key="Lookup Attribute Selector" description-key="A drop down control to choose a lookup attribute within the selected entity." control-type="virtual">
    <!-- property node identifies a specific, configurable piece of data that the control expects from CDS -->
   <property name="Attribute" display-name-key="Lookup" description-key="Lookup (single)" of-type="SingleLine.Text" usage="bound" required="true" />
   <property name="Entity" display-name-key="Entity" description-key="The name or field Name of the Entity we are displaying the lookups of." of-type="SingleLine.Text" usage="input" required="true" />  

    <resources>
      <code path="index.ts" order="1"/>
      <platform-library name="React" version="16.14.0" />
      <platform-library name="Fluent" version="9.46.2" />
    </resources>
  </control>
</manifest>
